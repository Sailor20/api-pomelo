{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "func-names": ["warn", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_id", "_json", "__channelService__"] }],
    "no-param-reassign": ["error", { "props": false }],
    "comma-dangle": ["error", "only-multiline"],
    "global-require": 0
  }
}
